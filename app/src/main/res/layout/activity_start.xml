<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="tmu2018.trunkcarassistant.StartActivity">

    <tmu2018.trunkcarassistant.TrunkView
        android:id="@+id/trunkView"
        android:layout_width="match_parent"
        android:layout_height="325dp" />

    <Button

        android:id="@+id/packit_start"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/PACK" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">



    </LinearLayout>

    <TextView
        android:id="@+id/carTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/carTextView" />

    <TextView
        android:id="@+id/carIDTextView"
        android:layout_width="72dp"
        android:layout_height="wrap_content" />


</LinearLayout>
